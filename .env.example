APP_NAME=skill-sync
APP_ENV=development
HTTP_PORT=8080

# DEV DB (untuk run server biasa)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=skill_sync
DB_USER=postgres
DB_PASSWORD=password
DB_SSL_MODE=disable
DB_RUN_SEEDERS=true

# JWT untuk DEV server
JWT_ACCESS_SECRET==
JWT_REFRESH_SECRET==
JWT_ACCESS_EXPIRES_IN=
JWT_REFRESH_EXPIRES_IN

# TEST DB (khusus integration test)
SKILLSYNC_TEST_DB_HOST
SKILLSYNC_TEST_DB_PORT
SKILLSYNC_TEST_DB_NAME
SKILLSYNC_TEST_DB_USER
SKILLSYNC_TEST_DB_PASSWORD
SKILLSYNC_TEST_DB_SSL_MODE

# (Opsional) JWT secret khusus test (kalau mau eksplisit; kalau tidak di-set, test pakai default internal)
SKILLSYNC_TEST_JWT_ACCESS_SECRET
SKILLSYNC_TEST_JWT_REFRESH_SECRET

# Redis cache (opsional; kalau tidak tersedia, API tetap jalan tanpa cache)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_TTL=600